{
    "rules": {
        "no-console": 0,
        "no-debugger": 0,
        "no-unexpected-multiline": 2,
        "indent": [2, 4],
        "quotes": [2, "single"],
        "semi": [2, "always"],
        "strict": [2, "global"],
        "curly": 2,
        "camelcase": [2, {"properties": "always"}],
        "eqeqeq": [2, "smart"],
        "no-extend-native": 2,
        "guard-for-in": 2,
        "wrap-iife": [2, "inside"],
        "new-cap": 2,
        "no-caller": 2,
        "no-eval": 2,
        "no-use-before-define": 2,
        "no-inner-declarations": [2, "both"],
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-shadow-restricted-names": 2,
        "no-shadow": 2,
        "new-parens": 2,
        "no-invalid-this": 2,
        "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
        "space-before-keywords": [2, "always"],
        "space-after-keywords": [2, "always"],
        "block-spacing": [2, "always"],
        "space-before-blocks": 2,
        "space-infix-ops": 2,
        "comma-spacing": [2, { "before": false, "after": true }],
        "array-bracket-spacing": [2, "never"],
        "space-unary-ops": [1, { "words": true, "nonwords": false }],
        "semi-spacing": [2, {"before": false, "after": true}],
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "eol-last": 2,
        "spaced-comment": [2, "always"],
        "dot-notation": 2,
        "no-lone-blocks": 2,
        "no-redeclare": 2,
        "no-with": 2,
        "global-require": 2,
        "comma-style": [2, "last"],
        "computed-property-spacing": [2, "never"],
        "key-spacing": 2,
        "newline-after-var": [2, "always"],
        "no-array-constructor": 2,
        "no-multiple-empty-lines": 2,
        "no-spaced-func": 2,
        "object-curly-spacing": [2, "always"],
        "space-in-parens": [2, "never"],
        "space-return-throw-case": 2
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jquery": true,
        "commonjs": true,
        "jasmine": true,
        "protractor": true
    },
    "extends": "eslint:recommended",

    "globals": {
        "TimelineLite" : false,
        "TimelineMax" : false,
        "TweenLite" : false,
        "TweenMax" : false,
        "Back" : false,
        "Bounce" : false,
        "Circ" : false,
        "Cubic" : false,
        "Ease" : false,
        "EaseLookup" : false,
        "Elastic" : false,
        "Expo" : false,
        "Linear" : false,
        "Power0" : false,
        "Power1" : false,
        "Power2" : false,
        "Power3" : false,
        "Power3" : false,
        "Power4" : false,
        "Quad" : false,
        "Quart" : false,
        "Quint" : false,
        "RoughEase" : false,
        "Sine" : false,
        "SlowMo" : false,
        "SteppedEase" : false,
        "Strong" : false,
        "Draggable" : false,
        "SplitText" : false,
        "VelocityTracker" : false,
        "CSSPlugin" : false,
        "ThrowPropsPlugin" : false,
        "BezierPlugin" : false,

        "THREE": false,
        "PIXI": false,
        "Phaser": false,
        "createjs": false,
        "Physics": false,

        "angular": false,
        "inject": false,

        "ga": false,
        "isMobile": false,
        "UAParser": false,
        "moment": false
    }
}
